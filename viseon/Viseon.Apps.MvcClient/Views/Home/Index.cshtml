@{
    ViewBag.Title = "Home Page";
}

<div class=" " style="min-height: 600px;">
    <div class="text-center">
    </div>
    <div class="row">
      
        <div class="col-md-12">
            <div class="text-center" style="max-width: 300px; margin: 0 auto; margin-top: 100px;">
                <img src="~/images/viseon-logo-300x100.png" />
               @Html.Partial("_SearchForm")
                @if (TempData["msg"] != null)
                {
                <div class="c-alert c-alert--danger alert">
                    <i class="c-alert__icon fa fa-times-circle"></i> @TempData["msg"]

                    <button class="c-close" data-dismiss="alert" type="button">×</button>
                </div>
                }
            </div>
        </div><!-- /.col-lg-6 -->
    </div><!-- /.row -->
</div>